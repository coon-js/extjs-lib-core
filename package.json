{
    "name": "lib-cn_core",

    "sencha": {

        "namespace": "coon.core",
        "type": "code",
        "creator": "coon.js",
        "summary": "Core functionality to be used with the coon.js Project",
        "detailedDescription": "This package contains extended and advanced implementations for ExtJS projects and provides a solid base for application building.",
        "version": "1.0.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "slicer": null,
        "output": {
            "base" : "${package.dir}/build",
            "js": {
                "version": "ES8"
            }
        },
        "language": {
            "js": {
                "input": "ES8",
                "output": "ES8"
            }
        },
        "compressor": {
            "type" : "closure",
            "polyfills": "none"
        },
        "local": true,
        "sass" : null,
        "classpath": [
            "${package.dir}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],
        "example": null,
        "framework": "ext",
        "requires": [
            "core",
            "package-loader"
        ]
    }
}
